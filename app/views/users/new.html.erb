New User Form

<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label :height %>
<%= f.number_field :height %>
<%= f.label :tickets %>
<%= f.number_field :tickets %>
<%= f.label "Mood" %>
<%= f.number_field :happiness %>
<%= f.label :nausea %>
<%= f.number_field :nausea %>
<%= f.label :admin %>
<%= f.check_box :admin %>
<%= f.submit "Create User" %>
<% end %>
